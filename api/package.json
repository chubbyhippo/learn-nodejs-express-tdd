    "config": "^3.3.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "sequelize": "^6.25.3",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
