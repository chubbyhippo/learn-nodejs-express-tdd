{
  "name": "express-ws",
  "version": "1.0.0",
  "description": "tdd with express",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest --watch",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chubbyhippo/learn-nodejs-express-tdd.git"
  },
  "author": "ch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chubbyhippo/learn-nodejs-express-tdd/issues"
  },
